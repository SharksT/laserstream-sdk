{
  "name": "helius-laserstream",
  "version": "0.0.7",
  "description": "High-performance Solana gRPC streaming client using Rust NAPI bindings with enhanced features",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "build:ts": "tsc",
    "example:account-sub": "ts-node examples/account-sub.ts",
    "example:slot-sub": "ts-node examples/slot-sub.ts",
    "example:transaction-sub": "ts-node examples/transaction-sub.ts",
    "example:transaction-status-sub": "ts-node examples/transaction-status-sub.ts",
    "example:block-sub": "ts-node examples/block-sub.ts",
    "example:block-meta-sub": "ts-node examples/block-meta-sub.ts",
    "example:entry-sub": "ts-node examples/entry-sub.ts",
    "perf:bandwidth": "ts-node performance/rust-bandwidth-test.ts",
    "perf:pure": "ts-node performance/pure-rust-bandwidth-test.ts",
    "perf:profile": "./performance/remote-rust-profile.sh",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "laserstream",
    "yellowstone",
    "grpc",
    "blockchain"
  ],
  "author": "Helius",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helius-labs/laserstream.git"
  },
  "bugs": {
    "url": "https://github.com/helius-labs/laserstream/issues"
  },
  "homepage": "https://github.com/helius-labs/laserstream/tree/main/javascript#readme",
  "files": [
    "index.js",
    "index.d.ts", 
    "laserstream-napi.*.node",
    "examples/",
    "README.md"
  ],
  "napi": {
    "name": "laserstream-napi",
    "triples": {
      "defaults": true,
      "additional": [
        "x86_64-pc-windows-msvc",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.2",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "@types/bs58": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.12",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
